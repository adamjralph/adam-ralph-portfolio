---
import MainHead from "../components/MainHead.astro";
import Header from "../components/Header.astro";
import Footer from "../components/Footer.astro";
const { title, description } = Astro.props;
---

<html lang="en">
  <head>
    <MainHead
      title={title}
      description={description}
    />
  </head>
  <body>
    <Header />
    <slot />
    <Footer />
  </body>
</html>
